  },
  "dependencies": {
    "get-image-colors": "^1.8.1",
    "micro": "^7.3.2",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.0.1",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.2.2",
    "serve": "^5.1.4",
    "test-listen": "^1.0.2"
  }
