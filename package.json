{
  "name": "micro-color-palette",
  "author": "xavcz (@xav_cz)",
  "licence": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "./node_modules/.bin/async-to-gen src --out-dir dist",
    "dev": "NODE_ENV=development nodemon --config package.json --exec micro",
